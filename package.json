{
  "name": "is-equal-type",
  "version": "0.0.2",
  "description": "A module to deeply compare two values of an argument to evaluate if they are of the same type",
  "keywords": [
    "javascript",
    "typescript",
    "module",
    "utility"
  ],
  "homepage": "https://github.com/kqito/is-same-type#readme",
  "bugs": {
    "url": "https://github.com/kqito/is-same-type/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kqito/is-same-type.git"
  },
  "license": "MIT",
  "author": "Nakagawa kaito",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "npm-run-all -s clean build:rollup",
    "build:rollup": "rollup -c",
    "clean": "rimraf ./dist",
    "format": "npm-run-all -s format:*",
    "format:fix": "prettier --write './src/**/*.{ts,json}'",
    "format:lint": "eslint ./src --ext .ts,.json",
    "prepublishOnly": "yarn build",
    "release": "np",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test",
      "...": "..."
    }
  },
  "lint-staged": {
    "*.{ts, js, json}": [
      "prettier --write",
      "jest",
      "git add"
    ]
  },
  "jest": {
    "testMatch": [
      "**/*.test.ts"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  },
  "dependencies": {
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@rollup/plugin-babel": "^5.0.4",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@types/jest": "^26.0.3",
    "@types/lodash": "^4.14.157",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "np": "^6.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.19.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.6"
  }
}
